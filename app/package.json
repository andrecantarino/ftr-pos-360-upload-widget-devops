  "devDependencies": {
    "@types/node": "22.10.0",
    "tsx": "4.19.2",
    "tsup": "^8.3.5",
    "typescript": "5.7.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.700.0",
    "@aws-sdk/lib-storage": "3.700.0",
    "@fastify/cors": "10.0.1",
    "@fastify/multipart": "9.0.1",
    "fastify": "5.1.0",
    "zod": "3.23.8"
  }
